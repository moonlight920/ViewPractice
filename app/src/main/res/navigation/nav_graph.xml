<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/nav_graph"
            app:startDestination="@id/naviLevel1Num1Fragment">

    <fragment android:id="@+id/naviLevel1Num1Fragment"
              android:name="com.yf.viewpractice.navigation.NaviLevel1Num1Fragment"
              android:label="fragment_navi_level1_num1" tools:layout="@layout/fragment_navi_level1_num1">
        <action android:id="@+id/action_naviLevel1Num1Fragment_to_naviLevel2Num1Fragment"
                app:destination="@id/naviLevel2Num1Fragment" app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim" app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment android:id="@+id/naviLevel2Num1Fragment"
              android:name="com.yf.viewpractice.navigation.NaviLevel2Num1Fragment"
              android:label="fragment_navi_level2_num1" tools:layout="@layout/fragment_navi_level2_num1">
        <action android:id="@+id/action_naviLevel2Num1Fragment_to_cupFragment" app:destination="@id/cupFragment"/>
        <argument android:name="dataA" app:argType="string" android:defaultValue="default"/>
    </fragment>
    <fragment android:id="@+id/cupFragment" android:name="com.yf.viewpractice.customview.cup.CupFragment"
              android:label="fragment_cup" tools:layout="@layout/fragment_cup"/>
</navigation>